    "lib": "lib"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "worker": "node src/workers/index.js",
    "dev-worker": "nodemon src/workers/index.js",
    "exam-processor": "node src/workers/exam-processor.js",
    "dev-exam-processor": "nodemon src/workers/exam-processor.js",
    "dev-all": "concurrently \"npm run dev\" \"npm run dev-worker\" \"npm run dev-exam-processor\"",
    "pm2-dev": "pm2 start ecosystem.config.cjs --env development",
    "pm2-prod": "pm2 start ecosystem.config.cjs --env production",
    "pm2-stop": "pm2 stop ecosystem.config.cjs",
    "pm2-logs": "pm2 logs",
    "lint": "eslint . --ext .js"
  },
